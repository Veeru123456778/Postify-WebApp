{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/index.js"
    },
    {
      "src": "/temp/(.*)",
      "dest": "/public/temp/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
